<div class="box" style="width:500px;background-color: #F0F0F0;">
    <h2>History of file</h2>
    <form name="historyform" method="post" action="FileHistoryDetails">
        <table>
            <tr>
                <td><h3>File ID</h3></td>
                <td>
                     <input type="text" id="fileId" name ="fileId" class="textbox" value="" style="width:200px;"/>
                </td>
            </tr>
            <tr>
                <td><h3>From:</h3></td>
                <td>
                    <div id="SDater">
                        <input type="text" id="date1" name ="Sdate" value="" style="width:200px;"/>
                    </div>
                </td>
            </tr>
            <tr>
                <td><h3>To:</h3></td>
                <td>
                    <div id="Edater">
                        <input type="text" id="date2" name ="Edate" value="" style="width:200px;"/>
                    </div>
                </td>
            </tr>
        </table> 
        <input type="submit"  value="Display" class="blue button"/>
    </form>
</div>
<script>
    $("#date1").kendoDatePicker({
        format:"yyyy-MM-dd"
    }); 
    $("#date2").kendoDatePicker({
        format:"yyyy-MM-dd"
    }); 
</script>