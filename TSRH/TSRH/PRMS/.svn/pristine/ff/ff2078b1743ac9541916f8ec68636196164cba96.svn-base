<div class="box" style="width:500px;background-color: #F0F0F0;">
    <form name="todayfiles" action="/TSRH/" method="post">
        <h2>Search for a file: <input type="submit" value="Pending Files" class="blue button" style="float:right"/></h2>	 
    </form>	

    <form name="searchfileform" action="SearchResult" method="post">
        <table style="border: 0;">
            <tr>
                <td width="310" style="border: 0px none;">
                    <h3>File ID:</h3> 
                </td>
                <td style="border: 0px none;">
                    <input type="text" class="textbox" name="fileId"/>
                </td>
            </tr>
            <tr>			
                <td style="border: 0px none;">
                    <h3>Patient Name:(LastName,FirstName)</h3>
                </td>
                <td style="border: 0px none;">
                    <input type="text" class="textbox" name="patientName"/>
                </td>
            </tr>
            <tr>
                <td style="border: 0px none;">
                    <h3>Date of Birth:</h3> 
                </td>
                <td style="border: 0px none;">
                    <div id="example" class="k-content">
                        <input id="DOB" name ="DOB" value="" style="width:165px;" />
                    </div>
                </td>
            </tr>
        </table>		
        <input type="reset" class="blue button" value="Reset"  />       
        <input type="submit" class="blue button" value="Submit" />
    </form>
</div>
