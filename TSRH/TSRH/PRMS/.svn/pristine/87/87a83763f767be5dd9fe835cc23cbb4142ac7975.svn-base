
<div class="box" style="width:650px;background-color: #F0F0F0">
    <h2>Enter Patient ID Number To View Patient Record</h2>
    <form action="PatientProfile" method="post">
        <% String patient = "";%>
        <table align="center" border='0' style="border: 0;border-color: #FFFFFF">
            <tr style="text-align: left;vertical-align: central">
                <td style="border: 0;border-color: #FFFFFF">
                    <h3>Patient ID Number</h3>
                </td>
                <td style="border: 0;border-color: #FFFFFF">
                    <p>
                        <input name="patient" class="textbox" type="text" value="<%=patient%>" size="20">
                    </p>
                </td>
                 <td style="border: 0;border-color: #FFFFFF">
                    <input type="submit" name="submit" class="blue button" value="Search">
                </td>
            </tr>
        </table>

        <%session.setAttribute("patient", patient);%>

    </form>
</div>
